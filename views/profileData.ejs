                     <form name="profileInformation" class="form-horizontal" action="/profile" method="post">
                                                     
                        <div id="ProfileColumn1"  class="col-sm-6  setting colProfile">
                        <div style="width: 280px; float: right;">
                            <div class="form-group">
                                <label class="control-label">Name:</label>
                                <input type="text" class="form-controler" name="username" id="username" value="<%= user.local.name %>" /> 
                            </div>
                               
                            <div class="form-group">
                                <label class="control-label">Mobile:</label>
                                <input type="number" class="form-controler" name="mnumber" id="mnumber" value="<%= user.local.mobile %>">
                            </div>                                   
                            
                            <div class="form-group">
                                <div class="dropdown">
                                    <label class="control-label" for="text">Gender:</label>
                  				   <% var Gender = [
    "Man", "Woman"
    ];%>                                                                      
                                    <select class="form-controler edit selectGender" id="gender" name="gender"  ><span class="caretNext caretNext1 caret "></span>
					<% Gender.forEach( function( sex ){ %>
					
					                    <option valuw="<%= sex%>" <% if(profile.gender == sex) {%> selected="selected"<%}%> >
						 <%= sex%>
					                    </option>
					 <% });%>
                                    </select>                               
                                </div>        
                            </div>
                            <div class="form-group">
                                <label class="control-label" >Address:</label>
                                <textarea  class="form-controler addressSection2" rows="6" id="comment" type="text"  name="address" ng-model="address" ng-minlength="20" ng-maxlength="200" required/><%= profile.address %></textarea>
                                <span ng-show="profileInformation.address.$touched && profileInformation.address.$invalid "  style="    position: absolute;
    top: 8em;" >Address is required.</span>
                            </div>
                        </div>
                    </div>
                                                      
                      <div id="ProfileColumn2"  class="col-md-6 rightSetting">
                          
                          <div style="width: 280px; float: left">                                  
                          <div class="form-group Up">
                                                                   
                              <label class="control-label">Email:</label>
                                                                           
                                  <input type="email" class="form-controler" name="email" id="email" value=" <%= user.local.email %> "  >
                                                                
                          </div> 
                                                            
                                                            
              <div class="form-group Up">
                               <div style="float: left; margin-top: 14px">                                          
                              <label class="control-label">DOB:</label>
                                                                           
                              
                                    <% var monthDates = [
    "01", "02", "03",
    "04", "05", "06", "07",
    "08", "09", "10",
    "11", "12"
    ];%>      
								  
							
								  
                             <input id="calendar" type="date" value="<% if(profile.dob) {%><%= profile.dob.getFullYear(); %>-<%= monthDates[profile.dob.getMonth()]; %>-<%= profile.dob.getDate(); %><%}%>"
								  style="    width: 7.87em;    margin-top: -0.25em;"  name="calendar" class="form-controler" > 
                                                                          
                              </div>
                                                                   
                              <label class="control-label Ageing"style="width: 22px; line-height: 1px">Age:</label>
                                                                             
                              <div id="profileAge" style=""  maxvalue="2">
                                                                                
                                  <input type="number" id="age" value="<%= profile.age %>" name="age" class="form-controler Ageing1"
                                         style="margin-bottom: -1em; ;width: 2.35em;margin-left: 0.2em;    margin-top: -2em;"  ng-model="Age" ng-minlength="2" ng-maxlength="2" required/>
                                        <span ng-show="profileInformation.age.$touched && profileInformation.age.$invalid"  >Age is invalid(must be 18 and above).</span>                                         
                              </div>
                                         
                                                                
                          </div>
                                                            
                                                            
                                                            
                                                             
                          <div class="form-group Up">
                                                                        
                              <div class="dropdownMarital"style="">
                                                                           
                                  <label class="control-label">Marital Status:</label>
                            <% var MaritailStatus = [
    "Single", "Married","Divorced"
    ];%>                                                                      
                 <select class="form-controler edit selectGender" id="maritalstatus" name="maritalstatus" ><span class="caretNext caretNext1 caret "></span>
					<% MaritailStatus.forEach( function( status ){ %>
					
					 <option valuw="<%= status%>" <% if(profile.marital_status == status) {%> selected="selected"<%}%> >
						 <%= status%>
					 </option>
					 <% });%>
                            </select>
                                  
                                                                    
                              </div>        
                                                              
                          </div>     
                         
                                                                    
                          <div class="form-group Up">
                                                                        
                              <label class="control-label"  >PAN:</label>                 
                                  <input type="text" value="<%= profile.pan %>" class="form-controler" name="pan" id="pan"
                                         ng-model="PanNumber" ng-pattern=
           "/^[\+]?[(]?[a-zA-Z]{5}[)]?[-\s\.]?[0-9]{4}[-\s\.]?[a-zA-z]{1}$/"
           required/>  <span ng-show="profileInformation.pan.$touched && profileInformation.pan.$invalid "  >invalid PAN.</span> 
                                                                        
                          </div>
                               
                                                              
                                                            
                                                            
                                                                                        
                                                                    
                                                        
                                                            
                                                                    
                       <div class="form-group Up">
                                                                   
                              <label class="control-label">City:</label>           
                                  <input type="text" class="form-controler" id="city" name="city" value="<%= profile.city %>"   ng-model="userCity" required/> 
                                      <span ng-show="profileInformation.city.$touched && profileInformation.city.$invalid"  >City is required.</span>                  
                          </div> 
                     <div class="form-group Up">
                                                                  
                                   <label class="control-label" for="pwd">Pincode:</label>
                                                                                    
                                    <input type="number" value="<%= profile.pincode %>" name="pincode" id="pincode" class="form-controler" ng-model="PinCode" ng-pattern=
           "/^[\+]?[(]?[1-9]{1}[)]?[-\s\.]?[0-9]{5}$/"
           required/ > 
                         <span ng-show="profileInformation.pincode.$touched && profileInformation.pincode.$invalid "  >Pincode is required.</span>      
                                              </div>
                                                     
                                                
                      </div>

                                            
                                                         </div>
                                                         </div>
                     <button type="submit" class="btn btn-lg moodNav profileSubmit" ng-disabled="profileInformation.$invalid" style="margin-top: 25px" >Submit </button>
                                                         </form>
                 <br>
                 
                           <h2 class="profilePan"style="position: relative; top: 90px;">Pan Details</h2>   
                </div>

    

                     <form name="profileInformation" class="form-horizontal" action="/profile" method="post">
                                                     
                         <div id="ProfileColumn1"  class="col-sm-6  setting colProfile">
                                                         
                             <div class="form-group">
                                                               
                                 <label class="control-label  col-sm-2 colo"style="width: 5em;" >Name:</label>
                                                                        
                                 <div class="col-md-3">
                                  <input type="text" class="form-controler" name="username" id="username" value="<%= user.local.name %>" /> 
                                                                    
                                 </div>
                                            <br>
                                                            </div>     
                               
                               
                                <div class="form-group">
                                    <label class="control-label  col-sm-2 colo" style="width: 5em;">Mobile:</label>

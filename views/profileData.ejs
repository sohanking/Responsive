                                     <textarea  class="form-controler addressSection2" rows="6" id="comment" type="text"  name="address" ng-model="address" ng-minlength="20"
                                ng-maxlength="200"              
                                           required/><%= profile.address %></textarea>  
                            <span ng-show="profileInformation.address.$touched && profileInformation.address.$invalid "  style="    position: absolute;
    top: 8em;" >Address is required.</span>   
                                                  
                            </div>
                                           
                            <br>
                                                                
                          
                                           
                    </div>     
                                                         
                                                                                      
                                                                
                                         
                      </div>
                          
                           
                                                      

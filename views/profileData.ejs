                                       <input type="number" class="form-controler" name="mnumber" id="mnumber" value="<%= user.local.mobile %>">
                                             
                                                </div>
                                           <br>
                                                            </div>
                               
                               
                               
                                       
                  <div class="form-group">
                                          <div class="dropdown">
                     <label class="control-label  col-sm-2 colo" for="text" style="width: 5em;" >Gender:</label>
                                                                               
                                              
                 <div class="col-md-3 collum">
                  				   <% var Gender = [
    "Man", "Woman"
    ];%>                                                                      
                 <select class="form-controler edit selectGender" id="gender" name="gender"  ><span class="caretNext caretNext1 caret "></span>
					<% Gender.forEach( function( sex ){ %>
					
					 <option valuw="<%= sex%>" <% if(profile.gender == sex) {%> selected="selected"<%}%> >
						 <%= sex%>
					 </option>
					 <% });%>
                            </select>
                                              
                                                                                                 
                             </div>
                                            <br>
                                  
                             
                                                                   
                         </div>        
                                                                
                                              
                                              </div>     
                               
                               
                                                               
                                              
                        <div class="form-group">
                                                                   
                                                  
